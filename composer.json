{
    "name": "maxinyugh/laravel-authz",
    "keywords": [
        "laravel",
        "casbin",
        "permission",
        "access-control",
        "authorization",
        "rbac",
        "acl",
        "abac",
        "authz"
    ],
    "description": "An authorization library that supports access control models like ACL, RBAC, ABAC in Laravel. ",
    "authors": [
        {
            "name": "maxinyu",
            "email": "408567964@qq.com"
        }
    ],
    "license": "Apache-2.0",
    "require": {
        "maxinyugh/php-casbin": "~1.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7|~6.0|~7.0|~8.0",
        "php-coveralls/php-coveralls": "^2.1",
        "mockery/mockery": "^1.0",
        "laravel/laravel": "~5.1"
    },
    "autoload": {
        "psr-4": {
            "Lauthz\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lauthz\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lauthz\\LauthzServiceProvider"
            ],
            "aliases": {
                "Enforcer": "Lauthz\\Facades\\Enforcer"
            }
        }
    }
}